{
  "name": "circle-api",
  "version": "1.0.0",
  "description": "API Service for Circle App.",
  "main": "dist/src/app.js",
  "repository": "https://github.com/andry-pebrianto/circle-api.git",
  "author": "Andry Pebrianto <andrypeb227@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "start": "node dist/src/app.js",
    "worker": "nodemon dist/src/worker/index.js",
    "worker:dev": "nodemon src/worker/index.ts",
    "migrate:generate": "npx typeorm-ts-node-commonjs migration:generate -d database/data-source.ts database/migration/MigrationFile",
    "migrate:up": "npx typeorm-ts-node-commonjs migration:run -d database/data-source.ts",
    "migrate:down": "npx typeorm-ts-node-commonjs migration:revert -d database/data-source.ts",
    "compile": "npx tsc",
    "lint": "eslint . --ignore-path .eslintignore --ext .ts",
    "lint:fix": "eslint . --ignore-path .eslintignore --ext .ts --fix"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.3",
    "@types/bcrypt": "^5.0.1",
    "@types/cors": "^2.8.14",
    "@types/eslint": "^8.44.4",
    "@types/express": "^4.17.19",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/lodash": "^4.14.200",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.8.4",
    "@types/node-cron": "^3.0.10",
    "@types/pg": "^8.10.5",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "sleep-promise": "^9.1.0",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.1"
  }
}
